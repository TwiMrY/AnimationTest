.fave {
	background: url(data:image/png;base64,) no-repeat;
	background-position: 0 0;
	width: 70px;
	height: 50px;
	cursor: pointer;
}
.fave.active {
	background-position: -3519px 0;
	-webkit-transition: backgroud 0.8s steps(55);
	-o-transition: backgroud 0.8s steps(55);
	transition: backgroud 0.8s steps(55);
}

